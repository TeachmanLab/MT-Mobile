{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --silent -u --force-exit",
    "testWatch": "jest --watchAll"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|sentry-expo|native-base)"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(jsx)$",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "collectCoverage": true,
    "coverageDirectory": "./__tests__",
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/apiServices.jsx",
      "!**/Information/About.jsx",
      "!**/Information/Debrief.jsx",
      "!**/Information/ExitQuestionnaire.jsx",
      "!**/Information/ExitStudy.jsx",
      "!**/AccountComponents/**",
      "!**/FormlyComponents/WordFill.jsx",
      "!**/FormlyComponents/ImagePage.jsx",
      "!**/FormlyComponents/YesNo.jsx",
      "!**/KeyboardShift.jsx",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/__tests__/**",
      "!**/babel.config.js",
      "!**/jest.setup.js",
      "!**/Navigation.jsx",
      "!**/App.jsx",
      "!**/components/Json/**",
      "!**/components/Progress.jsx",
      "!**/assets/**"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.5.1",
    "@material-ui/icons": "^4.5.1",
    "@react-native-community/slider": "^2.0.8",
    "@use-expo/font": "^2.0.0",
    "es6-promise": "^4.2.8",
    "expo": "^35.0.0",
    "expo-font": "^7.0.0",
    "expo-secure-store": "~7.0.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.15",
    "react": "^16.8.3",
    "react-dom": "16.8.3",
    "react-native": "https://github.com/expo/react-native/archive/sdk-35.0.0.tar.gz",
    "react-native-collapsible": "^1.5.1",
    "react-native-elements": "^1.2.6",
    "react-native-formly": "^1.0.0",
    "react-native-formly-templates-md": "^0.2.4",
    "react-native-gesture-handler": "~1.3.0",
    "react-native-modal-selector": "^1.1.4",
    "react-native-progress": "^4.1.2",
    "react-native-read-more-text": "^1.1.0",
    "react-native-reanimated": "^2.10.0",
    "react-native-screens": "~1.0.0-alpha.23",
    "react-native-slider": "^0.11.0",
    "react-native-vector-icons": "^6.0.0",
    "react-native-web": "^0.11.7",
    "react-navigation": "^4.0.10",
    "react-navigation-drawer": "^2.3.3",
    "react-navigation-stack": "^1.10.3",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "babel-preset-expo": "^7.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-node": "^4.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest-expo": "^35.0.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.8.3"
  },
  "private": true
}
